{
  "expo": {
    "name": "Luxe MedSpa",
    "slug": "luxe-medspa-patient",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "luxemedspa",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#667eea"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.luxemedspa.patient",
      "buildNumber": "1",
      "infoPlist": {
        "NSCameraUsageDescription": "We need camera access to take before/after treatment photos",
        "NSPhotoLibraryUsageDescription": "We need photo library access to save and view your treatment photos",
        "NSFaceIDUsageDescription": "Use Face ID for secure, passwordless login",
        "NSLocationWhenInUseUsageDescription": "We use your location to show nearby clinic locations",
        "NSUserNotificationsUsageDescription": "We send you appointment reminders, messages, and exclusive offers",
        "UIBackgroundModes": ["remote-notification"]
      },
      "associatedDomains": [
        "applinks:portal.luxemedspa.com",
        "webcredentials:portal.luxemedspa.com"
      ],
      "usesAppleSignIn": true,
      "config": {
        "usesNonExemptEncryption": false
      },
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#667eea"
      },
      "package": "com.luxemedspa.patient",
      "versionCode": 1,
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "USE_BIOMETRIC",
        "USE_FINGERPRINT",
        "VIBRATE",
        "RECEIVE_BOOT_COMPLETED",
        "POST_NOTIFICATIONS",
        "WAKE_LOCK"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "data": [
            {
              "scheme": "https",
              "host": "portal.luxemedspa.com",
              "pathPrefix": "/"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "server",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      "expo-apple-authentication",
      "expo-local-authentication",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Luxe MedSpa to access your camera for treatment photos"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Luxe MedSpa to access your photos for treatment documentation"
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/images/notification-icon.png",
          "color": "#667eea",
          "sounds": [
            "./assets/sounds/notification.wav"
          ],
          "mode": "production"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b8f3a2e1-4c5d-6e7f-8a9b-0c1d2e3f4a5b"
      }
    },
    "notification": {
      "icon": "./assets/images/notification-icon.png",
      "color": "#667eea",
      "androidMode": "default",
      "androidCollapsedTitle": "Luxe MedSpa"
    },
    "owner": "luxemedspa"
  }
}
